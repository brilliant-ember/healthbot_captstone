<?xml version="1.0" ?>
<robot name="healthbot">


 <gazebo>	
 <plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">
      <legacyMode>false</legacyMode>
      <alwaysOn>true</alwaysOn>
      <updateRate>20</updateRate>
      <leftJoint>Healthbot__left_wheel_hinge</leftJoint>
      <rightJoint>Healthbot__right_wheel_hinge</rightJoint>
      <wheelSeparation>0.4</wheelSeparation>
      <wheelDiameter>0.1</wheelDiameter>
      <torque>20</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>chassis</robotBaseFrame>
    </plugin>
  </gazebo>


<!-- Transmission is important to link the joints and the controller -->
           <transmission name="joint_w1_trans" type="SimpleTransmission">
                   <actuator name="joint_w1_motor" />
                  <joint name="Healthbot__left_wheel_hinge" />
                  <mechanicalReduction>1</mechanicalReduction>
                  <motorTorqueConstant>1</motorTorqueConstant>
         </transmission>

<transmission name="joint_w2_trans" type="SimpleTransmission">
                  <actuator name="joint_w2_motor" />
                 <joint name="Healthbot__right_wheel_hinge" />
                 <mechanicalReduction>1</mechanicalReduction>
                  <motorTorqueConstant>1</motorTorqueConstant>
          </transmission>


  <joint name="healthbot__chassis_JOINT_0" type="fixed">
    <parent link="healthbot__riser"/>
    <child link="healthbot__camera"/>
    <origin rpy="0       0      -0.00144" xyz="0.04984  0.00387  0.54136"/>
    <axis xyz="0  0  0"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>
